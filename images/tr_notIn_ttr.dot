digraph 
{
0 [shape = plaintext, label = ""]
1 [shape = box, label = "$X$", style = rounded];
2 [shape = box, label = "$0$", style = rounded];
1 -> 2[label = "$a$"]
1 -> 1[label = "$a$"]
0 -> 1

3 [shape = plaintext, label = ""]
4 [shape = box, label = "$X$", style = rounded];
4 -> 4[label = "$a$"]
3 -> 4
}